{
  "name": "tauri-tpl",
  "version": "1.0.0",
  "description": "A template of Tauri App for developers",
  "repository": "https://github.com/DingLipeng/tauri-tpl",
  "author": {
    "name": "Daniel Ting",
    "email": "danielting@aliyun.com",
    "url": "https://www.gznotes.com"
  },
  "type": "module",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "build:x64": "tauri build --target x86_64-pc-windows-msvc",
    "build:x64:verbose": "tauri build --target x86_64-pc-windows-msvc --verbose",
    "dev": "vite",
    "preview": "vite preview",
    "start": "tauri dev",
    "tauri": "tauri"
  },
  "lint-staged": {
    "package.json": "sort-package-json",
    "**/*.{js,jsx,ts}": [
      "eslint --fix"
    ],
    "*.{js,jsx,css,less,json,ts,vue}": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@fontsource/jetbrains-mono": "^5.0.8",
    "@tauri-apps/api": "^1.4.0",
    "element-plus": "^2.3.9",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.4.0",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.16.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.9",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.8.8"
  }
}
